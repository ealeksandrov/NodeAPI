    },
    "dependencies": {
        "body-parser": "^1.20.x",
        "express": "^4.19.x",
        "faker": "^5.1.0",
        "mongoose": "^6.12.x",
        "nconf": "^0.11.4",
        "oauth2orize": "^1.11.x",
        "passport": "^0.6.x",
        "passport-http": "^0.3.x",
        "passport-http-bearer": "^1.0.x",
        "passport-oauth2-client-password": "^0.1.x",
        "winston": "^3.2.x"
    },
    "devDependencies": {
        "superagent": "^6.1.0",
